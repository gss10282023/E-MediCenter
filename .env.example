# Copy to ".env" and fill in values.

# Django
DJANGO_DEBUG=1
# Generate one with: python3 -c "import secrets; print(secrets.token_urlsafe(50))"
DJANGO_SECRET_KEY=change-me
DJANGO_ALLOWED_HOSTS=0.0.0.0,localhost,127.0.0.1

# Third-party APIs (optional)
# If not set, Google Maps autocomplete + distance matching will be disabled.
GOOGLE_MAPS_API_KEY=

# If not set, /ask/ will reply with "OpenAI API key is not configured."
OPENAI_API_KEY=

